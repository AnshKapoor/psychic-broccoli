    "version": "0.2.0",
    "configurations": [
      
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "noise_data.xlsx",
                "adsb/data_2022_april.joblib",
                "--noise-output",
                "noise_with_matches.csv",
                "--traj-output",
                "matched_trajs.parquet",
                "--tol-sec",
                "10",
                "--buffer-frac",
                "0.5",
                "--window-min",
                "3",
                "--test-mode",
                "true",
                "--test-mode-match-limit",
                "5"
            ]
        },
        {
            "name": "Backbone Clustering (Test Mode)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/cli.py",
            "args": [
                "-c",
                "config/backbone.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Backbone Clustering (Full Mode)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/cli.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Visualize Outputs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/visualize_outputs.py",
            "args": [
                "--experiment-prefix",
                "OPTICS_exp_4"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - OPTICS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - GLOBAL OPTICS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/experiments/global_optics.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - GLOBAL DBSCAN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/experiments/global_dbscan.yaml",
                "--preprocessed",
                "output/run/csv/preprocessed_OPTICS_exp_5.csv"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - GLOBAL HDBSCAN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/experiments/global_hdbscan.yaml",
                "--preprocessed",
                "output/run/csv/preprocessed_OPTICS_exp_5.csv"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - GLOBAL KMeans",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/experiments/global_kmeans.yaml",
                "--preprocessed",
                "output/run/csv/preprocessed_OPTICS_exp_5.csv"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - GLOBAL Agglomerative",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/experiments/global_agglomerative.yaml",
                "--preprocessed",
                "output/run/csv/preprocessed_OPTICS_exp_5.csv"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - DBSCAN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - HDBSCAN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - KMeans",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Experiment Runner - Agglomerative",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/runner.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Run Named Experiments Grid",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run_experiment_grid.py",
            "args": [
                "--grid",
                "experiments/named_experiments.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Save Preprocessed Data",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/save_preprocessed.py",
            "args": [
                "-c",
                "config/backbone_full.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Convert ADSB Joblib to Parquet (April)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/convert_adsb_joblib_to_csv.py",
            "args": [
                "--joblib",
                "adsb/data_2022_april.joblib",
                "--output-dir",
                "output/adsb_parquet",
                "--chunksize",
                "200000",
                "--compression",
                "snappy"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Plot Experiment Results",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/plot_experiment_results.py",
            "args": [
                "GLOBAL_AGGLO_exp_1",
                "--max-flights-per-cluster",
                "30"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Generate Backbone Tracks",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/plot_experiment_results.py",
            "args": [
                "EXP46_optics_euclid_ms8_xi03_mcs06"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ],
    "compounds": [
        {
            "name": "Global Experiments (All Methods)",
            "configurations": [
                "Experiment Runner - GLOBAL OPTICS",
                "Experiment Runner - GLOBAL DBSCAN",
                "Experiment Runner - GLOBAL HDBSCAN",
                "Experiment Runner - GLOBAL KMeans",
                "Experiment Runner - GLOBAL Agglomerative"
            ],
            "stopAll": true
        }
    ]
}
